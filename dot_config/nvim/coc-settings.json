{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "Lua.telemetry.enable": false,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "StatusLine",
    "borderhighlight": "DiagnosticHint"
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "StatusLine",
    "borderhighlight": "DiagnosticHint"
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "StatusLine",
    "borderhighlight": "DiagnosticInfo"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "StatusLine",
    "borderhighlight": "DiagnosticWarn"
  },
  "snippets.ultisnips.enable": false
}
